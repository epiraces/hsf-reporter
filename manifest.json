{
  "manifest_version": 2,
  "name": "HSF Reporter",
  "version": "0.2",

  "icons": {
    "48": "img/icon48.png"
  },

  "browser_action": {
    "default_icon": "img/icon48.png",
    "default_popup": "html/reporter.html"
  },

  "background": {
    "scripts": ["lib/jquery-2.1.1.min.js" ,"js/background.js"]
  },

  "permissions": [
    "activeTab",
      "tabs",
      "storage",
    "*://bravenewtech.org/",
      "*://*.google.com/"
  ]
}
